{
  "name": "questionnaire-ui",
  "version": "0.0.1",
  "description": "Questionnaire UI for HelpMee",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config docs/webpack.config.js",
    "build": "rm -rf dist && NODE_ENV=production webpack",
    "build-docs": "rm -rf docs/build && NODE_ENV=production webpack --config docs/webpack.config.js",
    "lint": "eslint --ext .js,.vue --fix src __tests__; exit 0",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie/questionnaire-ui.git"
  },
  "keywords": [
    "spatie"
  ],
  "author": "François Lévesque",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie/questionnaire-ui/issues"
  },
  "homepage": "https://github.com/spatie/questionnaire-ui",
  "peerDependencies": {
    "axios": "^0.18",
    "buefy": "^0.7.3",
    "vue": "^2.3.0"
  },
  "devDependencies": {
    "axios": "^0.18",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "buefy": "^0.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.7.1",
    "eslint-config-spatie": "^1.1.0",
    "jest": "^19.0.0",
    "jest-vue-preprocessor": "^0.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.3",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.3.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "testRegex": "test.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }
}
